{
	"name": "chillerlan/php-oauth",
	"description": "A PHP7.2+ OAuth1/2 and API client",
	"homepage": "https://github.com/chillerlan/php-oauth",
	"license": "MIT",
	"type": "library",
	"minimum-stability": "dev",
	"keywords": [
		"oauth", "oauth1", "oauth2", "client", "core", "psr-7", "psr-17", "psr-18", "rfc5849", "rfc6749"
	],
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-oauth/issues",
		"source": "https://github.com/chillerlan/php-oauth"
	},
	"require": {
		"php": "^7.2",
		"ext-sodium": "*",
		"psr/log": "^1.0",
		"chillerlan/php-cache": "^3.0",
		"chillerlan/php-database": "^3.1",
		"chillerlan/php-dotenv": "^1.0",
		"chillerlan/php-oauth-providers": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.2"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\OAuthApp\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\OAuthAppCLI\\": "cli/",
			"chillerlan\\OAuthAppTest\\": "tests/",
			"chillerlan\\OAuthAppExamples\\": "examples/",
			"chillerlan\\OAuthTest\\": "vendor/chillerlan/php-oauth-core/tests"
		},
		"files": [
			"cli/cli_functions.php"
		]
	}
}
