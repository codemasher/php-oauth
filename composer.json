{
	"name": "chillerlan/php-oauth",
	"description": "An Oauth library for PHP7+.",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"oauth", "oauth2", "client", "spotify", "last.fm", "twitter", "discogs", "soundcloud", "gitter", "instagram", "github", "twitch", "vimeo"
	],
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net"
		}
	],
	"require": {
		"php": ">=7.0.7",
		"chillerlan/php-traits": "1.1.*",
		"psr/simple-cache": "1.0.0"
	},
	"require-dev": {
		"chillerlan/php-cache": "1.0.*",
		"chillerlan/php-curl": ">=1.0.3",
		"chillerlan/database": ">=1.0.2",
		"guzzlehttp/guzzle": "~6.0",
		"nikic/fast-route":"1.2.*",
		"phpunit/phpunit": "6.5.*"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\OAuth\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\OAuthCLI\\": "cli/",
			"chillerlan\\OAuthTest\\": "tests/",
			"chillerlan\\OAuthExamples\\": "examples/"
		}
	}
}
